                                            <fo:table-body>
                                                <fo:table-row background-color="#ffffff">
                                                    <fo:table-cell>
                                                        <fo:block margin-left="0.0cm" margin-right="0.0cm" margin-top="0.0cm" font-size="10pt" font-family="Arial" color="#000000" text-align="left">Další info:</fo:block>
